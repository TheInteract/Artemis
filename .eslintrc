{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "semi": ["warn", "never"],
        "linebreak-style": 0,
        "quotes": ["error", "single", { "allowTemplateLiterals": true }]
    }
}