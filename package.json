{
  "name": "collector",
  "version": "0.0.0",
  "description": "tracking user behavior",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "start:w": "npm run docker && nodemon",
    "build": "webpack",
    "build:prod": "webpack --config ./webpack.prod.config.js",
    "lint": "eslint src/** --ignore-path node_modules/",
    "test": "cross-env NODE_ENV=testing nyc --reporter=html --reporter=text mocha",
    "test:w": "cross-env NODE_ENV=testing mocha --watch",
    "docker:pull": "docker-compose -f docker/development.yml pull",
    "docker": "docker-compose -f docker/development.yml up -d",
    "stats": "webpack --config ./webpack.config.js --profile --json > stats.json",
    "stats:prod": "webpack --config ./webpack.prod.config.js --profile --json > stats.prod.json",
    "example": "http-server ./example -p 9999",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-register": "^6.18.0",
    "bluebird": "^3.4.7",
    "config": "^1.24.0",
    "cross-env": "^3.1.3",
    "kcors": "^2.2.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-router": "^7.1.0",
    "koa-send": "^3.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mongodb": "^2.2.20",
    "optimal-select": "^4.0.0",
    "redis": "^2.6.3",
    "sinon": "^1.17.7",
    "url-join": "^1.1.0",
    "whatwg-fetch": "^2.0.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "http-server": "^0.9.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "mongodb": "^2.2.20",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "webpack": "^2.1.0-beta.28"
  }
}
